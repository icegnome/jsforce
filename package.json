{
  "author": "Shinichi Tomita <shinichi.tomita@gmail.com>",
  "name": "jsforce",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com"
  ],
  "homepage": "http://github.com/jsforce/jsforce",
  "version": "2.0.0-alpha.11",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsforce/jsforce.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jsforce/jsforce/raw/master/LICENSE"
    }
  ],
  "main": "./index",
  "module": "./module/index",
  "types": "./lib/index",
  "scripts": {
    "clean": "rimraf lib/* module/*",
    "build:types": "run-p build:types:es5 build:types:module",
    "build:types:es5": "npm run build:types:src -- --outDir lib",
    "build:types:module": "npm run build:types:src -- --outDir module",
    "build:types:src": "tsc --emitDeclarationOnly --declaration -p src",
    "build": "run-p build:types build:es5 build:module",
    "build:es5": "SET NODE_ENV=production && npm run build:src -- -d lib",
    "build:module": "SET NODE_ENV=module && npm run build:src -- -d module",
    "build:src": "babel src --extensions \".ts\"",
    "lint": "eslint --ext .ts,.js src test",
    "typecheck": "tsc --noEmit",
    "test": "jest `if (test \"${SF_POOL_USERNAME}\" = \"\"); then echo \"-w 1\"; else echo \"-w 5\"; fi`"
  },
  "directories": {
    "test": "test/"
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "api",
    "bin",
    "build",
    "core",
    "lib",
    "src",
    "test",
    "index.d.ts",
    "index.js"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "bin": {
    "jsforce": "./bin/jsforce",
    "jsforce-gen-schema": "./bin/jsforce-gen-schema"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "base64url": "^3.0.1",
    "commander": "^4.0.1",
    "core-js": "^3.6.5",
    "csv-parse": "^4.12.0",
    "csv-stringify": "^5.5.1",
    "faye": "^1.4.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "multistream": "^4.0.1",
    "open": "^7.3.0",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/fs-extra": "^9.0.2",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/puppeteer": "^3.0.2",
    "@types/request": "^2.48.5",
    "@types/through2": "^2.0.36",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-preset-power-assert": "^3.0.0",
    "conditional-type-checks": "^1.0.5",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.1.2",
    "puppeteer": "^5.4.0",
    "rimraf": "^3.0.2",
    "through2": "^4.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "browser": {
    "./lib/registry": "./lib/browser/registry.js",
    "./lib/registry/index.js": "./lib/browser/registry.js",
    "./module/registry": "./module/browser/registry.js",
    "./module/registry/index.js": "./module/browser/registry.js",
    "request": "./lib/browser/request.js"
  }
}
